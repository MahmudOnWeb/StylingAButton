                                <EasingDoubleKeyFrame KeyTime="00:00:00.1500000" Value="0.7"/>
                                <EasingDoubleKeyFrame KeyTime="00:00:00.2000000" Value="0.9"/>
                                <EasingDoubleKeyFrame KeyTime="00:00:00.2500000" Value="1"/>
